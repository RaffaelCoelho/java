public class Professor {
    private String nome;
    private String cpf;
    private String materia;

    public Professor(String nome, String cpf, String materia) {
        this.nome = nome;
        this.cpf = cpf;
        this.materia = materia;
    }

    public String getNome() {
        return nome;
    }

    public String getCpf() {
        return cpf;
    }

    public String getMateria() {
        return materia;
    }
}
