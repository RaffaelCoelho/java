public class Aluno {
    private String nome;
    private String turma;
    private String cpf;

  
    public Aluno(String nome, String turma, String cpf) {
        this.nome = nome;
        this.turma = turma;
        this.cpf = cpf;
    }

    public String getNome() {
        return nome;
    }

    public String getTurma() {
        return turma;
    }

    public String getCpf() {
        return cpf;
    }
}
